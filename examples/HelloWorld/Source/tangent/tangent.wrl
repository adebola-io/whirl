module tangent;


public use basic.{button, text};
public use style;
public use events;
public use view;
public use actions;


public function horizontalView -> view.View {
    view.View()
        .style(style.style().layout(style.Layout.Horizontal))
}

public function verticalView -> view.View {
    view.View()
        .style(style.style().layout(style.Layout.Vertical))
}
