// !::no-prelude::!
module Internals;

use Abstract;


public function Invoke(
    command: Invocation,
    param1?: Abstract,
    param2?: Abstract,
    param3?: Abstract,
    param4?: Abstract,
    param5?: Abstract,
    param6?: Abstract,
    param7?: Abstract,
    param8?: Abstract,
    param9?: Abstract,
): Abstract {
    // Compiler built-in.
}

/// List of variants that can be called with Invoke().
public enum Invocation {
    // Strings.
    StrAt,
    StrAppend,
    StrChars,
    StrClear,
    StrContains,
    StrLength,
    StrReplace,
    StrReverse,
    StrSlice,
    StrSplit,
    StrTrim,
    StrTrimInPlace,
    StrTrimLeft,
    StrTrimRight,
    StrPad,
    StrPadLeft,
    StrPadInPlace,
    StrPrepend,
    StrUppercase,
    StrLowercase,
}